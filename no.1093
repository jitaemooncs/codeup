#include <stdio.h>

int main(void)
{
    int number, count;
    int students[23] = {};
    scanf("%d", &number);
    
    for (int i = 0; i < number; i++)
    {
        scanf("%d", &count);
        students[count - 1] += 1;
    }
    
    for (int i = 0; i < 23; i++)
    {
        printf("%d ", students[i]);
    }
}
