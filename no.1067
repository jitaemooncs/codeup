#include <stdio.h>

int main(void)
{
    int number;
    
    scanf("%d", &number);
    
    if (number > 0)
    {
        printf("plus\n");
        
        if (number % 2 == 0)
        {
            printf("even\n");
        }
        else
        {
            printf("odd\n");
        }
    }
    else
    {
        printf("minus\n");
        
        if (number % 2 == 0)
        {
            printf("even\n");
        }
        else
        {
            printf("odd\n");
        }
    }
}
